{
  "name": "disoc-floor-controller",
  "productName": "Disco Controller",
  "version": "2.0.0",
  "author": "Jeremy Gillick",
  "description": "Disco dance floor controller application.",
  "repository": "https://github.com/jgillick/DiscoDanceFloor",
  "license": "MIT",
  "main": "src/app.js",
  "devDependencies": {
    "css-loader": "^0.23.1",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "^0.37.7",
    "file-loader": "^0.8.5",
    "node-sass": "^3.5.3",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "font-awesome": "^4.6.1",
    "angular2": "^2.0.0-beta.16",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12"
  },
  "scripts": {
    "start": "ENVIRONMENT=DEV electron .",
    "build": "ENVIRONMENT=DIST webpack",
    "watch": "webpack-dev-server --hot --inline",
    "dist-osx": "ENVIRONMENT=DIST webpack && electron-packager . --out ./dist --platform=darwin --arch=x64 --overwrite --ignore=\"node_modules|dist\" --icon=./src/images/app_icon_grey.icns",
    "dist-linux": "ENVIRONMENT=DIST webpack && electron-packager . --out ./dist --platform=linux --arch=x64 --overwrite --ignore=\"node_modules|dist\" --icon=./src/images/app_icon_grey.icns"
  }
}
